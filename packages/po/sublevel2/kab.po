msgstr "Tuccḍa deg Debootstrap"

#. Type: error
#. Description
#. :sl2:
#: ../bootstrap-base.templates:3001
msgid "Failed to determine the codename for the release."
msgstr "I tifin n yisem n tengalt i lqem yecceḍ."

#. Type: error
#. Description
#. :sl2:
#: ../bootstrap-base.templates:4001
msgid "Failed to install the base system"
msgstr "Asebded n unagraw n taffa yecceḍ"

#. Type: error
#. Description
#. :sl2:
#: ../bootstrap-base.templates:4001
msgid "The base system installation into /target/ failed."
msgstr "Asebded n unagraw n taffa deg /target/ yecceḍ."

