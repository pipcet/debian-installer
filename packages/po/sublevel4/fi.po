#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:2001
msgid "KEYSTROKES:"
msgstr "NÄPPÄINOIKOTIET:"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:3001
#, no-c-format
msgid "'%c'"
msgstr "”%c”"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:4001
